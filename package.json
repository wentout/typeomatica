{
	"name": "typeomatica",
	"version": "0.2.5",
	"description": "type logic against javascript metaprogramming",
	"main": "lib/index.js",
	"scripts": {
		"build": "rm -rf ./lib && npx tsc --pretty",
		"buildlint": "rm -rf ./lib && npx tsc --pretty && npm run lint",
		"lint": "npx eslint --fix --ignore-path .gitignore ./lib",
		"test": "npx jest",
		"test:cov": "npx jest --collectCoverage",
		"debug": "npx --node-options=--inspect-brk jest"
	},
	"keywords": [
		"strict",
		"types",
		"runtime",
		"javascript",
		"metaprogramming"
	],
	"author": "went.out@gmail.com",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wentout/typeomatica.git"
	},
	"devDependencies": {
		"@types/jest": "^27.0.1",
		"@types/node": "^16.9.2",
		"eslint": "^7.32.0",
		"husky": "^7.0.2",
		"jest": "^27.2.0",
		"lint-staged": "^11.1.2",
		"set-value": "^4.1.0",
		"ts-jest": "^27.0.5",
		"ts-node": "^10.2.1",
		"tslint": "^6.1.3",
		"typescript": "^4.4.3"
	}
}
