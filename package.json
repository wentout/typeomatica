{
	"name": "typeomatica",
	"version": "0.0.3",
	"description": "type logic agains javascript metaprogramming",
	"main": "lib/index.js",
	"scripts": {
		"build": "rm -rf ./lib && npx tsc --pretty",
		"buildlint": "rm -rf ./lib && npx tsc --pretty && npm run lint",
		"lint": "npx eslint --fix --ignore-path .gitignore ./lib",
		"test": "npx jest",
		"test:cov": "npx jest --collectCoverage",
		"debug": "npx --node-arg=--inspect-brk jest"
	},
	"keywords": [
		"strict",
		"types",
		"runtime",
		"javascript",
		"metaprogramming"
	],
	"author": "went.out@gmail.com",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wentout/typeomatica.git"
	  },
	"devDependencies": {
		"@types/jest": "^26.0.22",
		"@types/node": "^14.14.37",
		"eslint": "^7.23.0",
		"husky": "^3.1.0",
		"jest": "^26.6.3",
		"ts-jest": "^26.5.4",
		"ts-node": "^9.1.1",
		"tslint": "^6.1.3",
		"typescript": "^4.2.3",
		"typologica": "0.0.77"
	},
	"dependencies": {
		"lint-staged": "^10.5.4"
	}
}
